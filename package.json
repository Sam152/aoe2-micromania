    "@types/lodash": "^4.14.74",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.139.0",
    "css-loader": "^6.7.1",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.1",
